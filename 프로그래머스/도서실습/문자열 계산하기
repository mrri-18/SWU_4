def solution(my_string):
    my_string = my_string.split()
    answer=int(my_string[0])
    for x in range(len(my_string)):
        if my_string[x] == "+":
            answer+=int(my_string[x+1])
        elif my_string[x] == "-":
            answer-=int(my_string[x+1])
            
    return answer
